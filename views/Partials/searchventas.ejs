<table class="table table-hover">
    <thead class="thead-dark">
    <tr>
        <th scope="col">#</th>
        <th scope="col">Amount</th>
        <th scope="col">Client</th>
    </tr>
    </thead>
    <tbody>
    <% for (let i = 0 ; i < type.length ;i++){ %>
        <tr>
            <th scope="row"><%- type[i].id %></th>
            <td><%- type[i].money %></td>
            <td><%- type[i].client.name %></td>
        </tr>
    <% } %>
    </tbody>
</table>
